{% extends 'base.html.twig'%}
{% block body %}
<h1>Modify my profile</h1>

{{ form_start(form) }}
{{ form_row(form.firstname) }}
{{ form_row(form.lastname)}}
{{ form_row(form.email)}}
{{ form_row(form.password)}}
{{ form_row(form.confirm_password)}}
<button type="submit" class="btn btn-success">MODIFY</button>
{{ form_end(form) }}
{% endblock %}